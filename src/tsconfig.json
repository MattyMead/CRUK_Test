{
  "compilerOptions": {
    "strict": true,
    "target": "es5",
    "lib": [
      "es5",
      "es6",
      "dom"
    ],
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "esModuleInterop": true,
    "removeComments": true,
    "incremental": true,
    "resolveJsonModule": true,
    "downlevelIteration": true,
    "baseUrl": ".",
    "paths": {
      "*": [
        "*"
      ]
    }
  }
}